<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="dish_name">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="dish_name_dialog">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="dish_price">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="dish_weight">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#66000000</item>
    </style>

    <style name="dish_description">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#A6000000</item>
    </style>

    <style name="dish_button_dialog">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">@color/button_background_color</item>
        <item name="android:background">@drawable/dialog_button_background</item>
    </style>






</resources>